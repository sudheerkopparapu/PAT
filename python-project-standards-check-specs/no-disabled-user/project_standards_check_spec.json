/* This file is the descriptor for the Custom python Project Standards Check template no-disabled-user */
{
  "meta": {
    "label": "Project must not have disabled users",
    "description": "Check that the project has no disabled users referenced or as owners",
    "icon": "fas fa-check-circle"
  },
  "tags": ["AUTOMATION"],
  "params": [
      {
          "name": "severity",
          "label": "Severity level",
          "type": "SELECT",
          "selectChoices" : [
              { "value": "1", "label": "LOWEST"},
              { "value": "2", "label": "LOW"},
              { "value": "3", "label": "MEDIUM"},
              { "value": "4", "label": "HIGH"},
              { "value": "5", "label": "CRITICAL"}
          ],
          "defaultValue" : "5",
          "mandatory": true
      }
  ]
}
